<h1>New Restaurant</h1>
<%= form_for @restaurant, html: {multipart: true} do |f| %>
  
  <%= f.label :image %>
  <br>
  <%= f.file_field :image %>

  <br>

  <%= f.label :name %>
  <%= f.text_field :name%>

  <%= f.label :rating %>
  <%= f.text_field :rating %>
  
  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.submit class: 'btn'%>

<% end %>